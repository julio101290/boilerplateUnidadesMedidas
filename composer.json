{
    "name": "julio101290/boilerplateunidadesmedidas",
    "description": "CodeIgniter4 Catalogo simple de unidades de medida",
    "keywords": [
        "codeigniter4",
        "backup",
        "companies",
        "mysql",
        "CRUD",
        "invoice",
        "authorization",
        "boilerplate"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "julio101920",
            "email": "juliocesar101290@hotmail.com"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "codeigniter4/framework": "^4.1",
        "codeigniter4/translations": "^4.0",
        "julio101290/auth": "^1.0",
        "julio101290/boilerplate": "^1.0",
        "julio101290/boilerplatelog": "^1.0.0",
        "julio101290/boilerplatecompanies": "^1.0.0",
        "tecnickcom/tcpdf": "^6.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1",
        "fakerphp/faker": "^1.13"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "julio101290\\boilerplateunidadesmedidas\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-update-cmd": [
            "@composer dump-autoload"
        ],
        "test": "phpunit --colors=always -vvv"
    }
}
